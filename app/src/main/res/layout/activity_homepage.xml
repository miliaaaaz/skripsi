<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Homepage">

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="415dp"
        android:layout_height="275dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#FAF0E4"
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/joshua_clay"
        android:layout_width="259dp"
        android:layout_height="51dp"
        android:gravity="center_horizontal|top"
        android:text="@string/joshua_clay"
        android:textAppearance="@style/joshua_clay"
        android:textSize="35sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="30dp" />

    <TextView
        android:id="@+id/days"
        android:layout_width="300dp"
        android:layout_height="88dp"
        android:gravity="center_horizontal|top"
        android:text="@string/days"
        android:textAppearance="@style/days"
        android:textSize="65sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="96dp" />

    <TextView
        android:id="@+id/until_our_w"
        android:layout_width="241dp"
        android:layout_height="37dp"
        android:gravity="center_horizontal|top"
        android:text="@string/until_our_w"
        android:textAppearance="@style/until_our_w"
        android:textSize="21sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="206dp" />

</androidx.constraintlayout.widget.ConstraintLayout>